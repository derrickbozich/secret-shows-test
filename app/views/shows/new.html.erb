<%= form_for @show do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :city_name %>
  <%= f.text_field :city_name %>
  <%= f.label :venue_name %>
  <%= f.text_field :venue_name %>
  <%= f.fields_for :artists do |artist_builder| %>
    <%= artist_builder.label :name %>
    <%= artist_builder.text_field :name %>
  <% end  %>


  <%= f.submit %>



<% end  %>
